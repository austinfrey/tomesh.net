---
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID://Toronto Mesh/Events//NONSGML v1.0//EN
BEGIN:VTIMEZONE
TZID:America/Toronto
X-LIC-LOCATION:America/Toronto
BEGIN:DAYLIGHT
TZOFFSETFROM:-0500
TZOFFSETTO:-0400
TZNAME:EST
DTSTART:19700329T020000
RRULE:FREQ=YEARLY;INTERVAL=1;BYDAY=-1SU;BYMONTH=3
END:DAYLIGHT
BEGIN:STANDARD
TZOFFSETFROM:-0400
TZOFFSETTO:-0500
TZNAME:CET
DTSTART:19701025T030000
RRULE:FREQ=YEARLY;INTERVAL=1;BYDAY=-1SU;BYMONTH=10
END:STANDARD
END:VTIMEZONE
METHOD:PUBLISH
{% for post in site.posts limit:20 %}BEGIN:VEVENT
UID:{{ post.date | date: "%Y%m%d" }}@tomesh.net
ORGANIZER;CN="Toronto Mesh":MAILTO:hello@tomesh.net
LOCATION:{{ post.location }}
SUMMARY:{{ post.title }}
URL:https://tomesh.net{{ post.url|strip }}
DESCRIPTION;ALTREP="{{ post.text|strip }}":{{ post.text|strip }}
DTSTART;TZID=America/Toronto:{{ post.date | date: "%Y%m%d" }}T{{ post.startTime | date:"%H%M%S" }}
DTEND;TZID=America/Toronto:{{ post.date | date: "%Y%m%d" }}T{{ post.endTime | date:"%H%M%S" }}
DTSTAMP;TZID=America/Toronto:{{ post.date | date: "%Y%m%d" }}T{{ post.date | date:"%H%M%S" }}
END:VEVENT
{% endfor %}END:VCALENDAR
